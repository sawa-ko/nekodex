{
	"name": "@kaname-png/nekodex",
	"version": "0.0.2",
	"description": "A simple maid bot that provides roleplay commands.",
	"main": "dist/main.js",
	"types": "dist/main.d.ts",
	"author": "@kaname-png",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"start": "node --experimental-specifier-resolution=node --enable-source-maps ./dist/main.js",
		"lint": "eslint src --ext ts --fix",
		"format": "prettier --write src/**/*.ts",
		"docs": "typedoc",
		"test": "jest",
		"test:watch": "jest --watch",
		"update": "yarn upgrade-interactive",
		"build": "swc src -d dist --copy-files --env-name='production'",
		"watch": "swc src -d dist -w --copy-files --env-name='development'",
		"sversion": "standard-version",
		"commit": "git-cz",
		"cz": "git-cz",
		"prepublishOnly": "yarn build",
		"prepare": "husky install .github/husky"
	},
	"dependencies": {
		"@kaname-png/revoltx": "^2.0.1",
		"@skyra/env-utilities": "^1.0.1",
		"colorette": "^2.0.19",
		"common-tags": "^1.8.2",
		"file-type": "^17.1.2",
		"formdata-node": "^4.3.3",
		"got": "^12.1.0",
		"pretty-ms": "^8.0.0",
		"revolt.js": "^6.0.3",
		"tslib": "^2.4.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.0.3",
		"@commitlint/config-conventional": "^17.0.3",
		"@sapphire/eslint-config": "^4.3.7",
		"@sapphire/prettier-config": "^1.4.3",
		"@sapphire/ts-config": "^3.3.4",
		"@swc/cli": "^0.1.57",
		"@swc/core": "^1.2.208",
		"@types/common-tags": "^1",
		"@types/node": "^17.0.31",
		"@types/ws": "^8.5.3",
		"@typescript-eslint/eslint-plugin": "^5.30.3",
		"@typescript-eslint/parser": "^5.30.3",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.19.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"pretty-quick": "^3.1.3",
		"standard-version": "^9.3.2",
		"typescript": "^4.7.4"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sapphiredev/sapphire-template.git"
	},
	"files": [
		"dist",
		"!dist/*.tsbuildinfo"
	],
	"engines": {
		"node": ">=v14.0.0",
		"npm": ">=7.0.0"
	},
	"keywords": [
		"@sapphire/template",
		"bot",
		"typescript",
		"ts",
		"yarn",
		"discord",
		"sapphire",
		"discordjs"
	],
	"bugs": {
		"url": "https://github.com/sapphiredev/sapphire-template/issues"
	},
	"homepage": "https://www.sapphirejs.dev",
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"resolutions": {
		"ansi-regex": "^5.0.1",
		"minimist": "^1.2.6",
		"jest-environment-jsdom": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.1.3.tgz",
		"jest-jasmine2": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.1.3.tgz"
	},
	"prettier": "@sapphire/prettier-config",
	"packageManager": "yarn@3.2.1"
}
